<!DOCTYPE html>
<html ng-app>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <section ng-controller='LoginController'>
	  <form ng-submit='verificaLogin()'>
	  <input type='text' placeholder='nome'  ng-model='login.nome' />
	  <input type='text' placeholder='senha' ng-model='login.senha' />
	  <input type='submit' value='Logar' />
	  </form>
	</section>

  </body>
</html>
